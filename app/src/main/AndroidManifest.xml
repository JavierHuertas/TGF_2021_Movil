<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.anteproyectoidea">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_logo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AnteproyectoIdea">
        <activity android:name=".logins.LoginEmpresa"></activity>
        <activity android:name=".EmpresaRegistrarLogin" />
        <activity android:name=".logins.login" />
        <activity android:name=".registro.Registrarse_actividad" />
        <activity android:name=".registro.Registro" />
        <activity android:name=".splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.AnteproyectoIdea.NoActionBar" />
    </application>

</manifest>